<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch=".."
           review="https://android-review.googlesource.com/" />
  <default revision="master"
           remote="aosp"
           sync-j="4" />

  <!-- BEGIN trusty core projects -->
  <project path="external/lk" name="trusty/lk/common" groups="trusty" >
    <copyfile src="makefile" dest="makefile" />
  </project>
  <project path="external/headers" name="trusty/external/headers" groups="trusty" />
  <project path="lib" name="trusty/lib" groups="trusty" />
  <project path="lk/trusty" name="trusty/lk/trusty" groups="trusty" />
  <!-- END trusty core projects -->

  <!-- BEGIN apps projects  -->
  <project path="app/sample" name="trusty/app/sample" groups="trusty" />
  <project path="app/keymaster" name="trusty/app/keymaster" groups="trusty" />
  <project path="app/gatekeeper" name="trusty/app/gatekeeper" groups="trusty" />
  <project path="app/storage" name="trusty/app/storage" groups="trusty" />
  <project path="app/avb" name="trusty/app/avb" groups="trusty" />
  <!-- END apps projects -->

  <!-- BEGIN platform projects -->
  <project path="external/googletest" name="platform/external/googletest" groups="trusty" />
  <project path="hardware/libhardware" name="platform/hardware/libhardware" groups="trusty" />
  <project path="system/keymaster" name="platform/system/keymaster" groups="trusty" />
  <project path="system/gatekeeper" name="platform/system/gatekeeper" groups="trusty" />
  <project path="external/openssl" name="platform/external/boringssl" groups="trusty" />
  <project path="system/iot/attestation" name="platform/system/iot/attestation" groups="trusty" />
  <project path="tools/repohooks" name="platform/tools/repohooks" groups="trusty" />
  <!-- END platform projects -->

  <!-- BEGIN trusty device projects -->
  <project path="device/arm/generic-arm64" name="trusty/device/arm/generic-arm64" groups="trusty" />
  <project path="device/arm/vexpress-a15" name="trusty/device/arm/vexpress-a15" groups="trusty" />
  <!-- END trusty device projects -->

  <!-- BEGIN trusty vendor projects -->
  <project path="vendor/google/aosp" name="trusty/vendor/google/aosp" groups="trusty" >
   <!-- Install lk_inc.mk into trusty root -->
    <copyfile src="lk_inc.mk" dest="lk_inc.mk" />
  </project>
  <!-- END trusty vendor projects -->

  <!-- BEGIN toolchains -->
  <project path="prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.8" name="platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.8" groups="trusty" />
  <project path="prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9" name="platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9" groups="trusty" />
  <project path="prebuilts/gcc/linux-x86/arm/arm-eabi-4.8" name="platform/prebuilts/gcc/linux-x86/arm/arm-eabi-4.8" groups="trusty" />
  <project path="prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9" name="platform/prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9" groups="trusty" />
  <project path="prebuilts/clang/host/linux-x86" name="platform/prebuilts/clang/host/linux-x86" groups="trusty" />
  <!-- END toolchains -->

  <!-- BEGIN aux external projects -->
  <project path="external/qemu" name="trusty/external/qemu" groups="trusty" />
  <!-- END aux external projects -->

  <repo-hooks in-project="platform/tools/repohooks" enabled-list="pre-upload" />
</manifest>
